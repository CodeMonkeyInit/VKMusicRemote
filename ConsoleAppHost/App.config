<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.serviceModel>
   
    <services>
      <service name="VKMusicRemote.Services.LoginService">

        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/LoginService"/>
          </baseAddresses>
        </host>
        
        <endpoint address="http://localhost:8080/LoginService" contract="VKMusicRemote.ServicesContracts.ILoginService" binding="basicHttpBinding">
        </endpoint>
       
      </service>

      <service name="VKMusicRemote.Services.MusicService">

        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/MusicService"/>
          </baseAddresses>
        </host>

        <endpoint address="http://localhost:8080/MusicService" contract="VKMusicRemote.ServicesContracts.IMusicService" binding="basicHttpBinding">
        </endpoint>
        
      </service>
      
    </services>

    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceDebug includeExceptionDetailInFaults="true"/>
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    
    <bindings/>
    <client/>
  </system.serviceModel>
</configuration>
